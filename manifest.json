{
  "manifest_version": 2,
  "name": "PDF Text Extractor",
  "version": "1.0",
  "description": "Extract text from PDFs on specific pages when hovering over them.",
  "permissions": [
    "activeTab",
    "https://pje.trt6.jus.br/*"  // Replace with the specific URL(s) you want to target
  ],
  "content_scripts": [
    {
      "matches": ["https://pje.trt6.jus.br/*"],  // Replace with the specific URL(s) you want to target
      "js": ["content.js"]
    }
  ]
}